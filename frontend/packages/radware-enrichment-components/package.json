{
  "name": "@splunk/radware-enrichment-components",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "build": "node bin/build.js build",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "yarn run eslint -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "link:app": "node bin/build.js link",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "start": "webpack --watch",
    "start:app": "webpack --watch --config demo/webpack.splunkapp.config.js",
    "start:demo": "node bin/build.js demo",
    "stylelint": "stylelint \"src/**/*.{js,jsx}\" --config stylelint.config.js",
    "test": "jest",
    "test:ci": "JEST_JUNIT_OUTPUT_DIR=./test-reports JEST_JUNIT_OUTPUT_NAME=unit-results.xml JEST_JUNIT_CLASSNAME=unit yarn run test --ci --reporters=default jest-junit --coverage --coverageDirectory=coverage_report/coverage_maps_unit --coverageReporters=cobertura",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch"
  },
  "main": "RadwareEnrichmentSetupComponent.js",
  "dependencies": {
    "@splunk/react-icons": "^3.3.1",
    "@splunk/react-time-range": "^8.1.2",
    "@splunk/react-toast-notifications": "^0.11.1",
    "@splunk/react-ui": "^4",
    "@splunk/search-job": "^2.0.1",
    "@splunk/themes": "^0.15.0",
    "async-sema": "^3.1.1",
    "lodash": "^4.17.21",
    "querystring-es3": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@splunk/babel-preset": "^3.0.0",
    "@splunk/eslint-config": "^4.0.0",
    "@splunk/splunk-utils": "^2.3.1",
    "@splunk/stylelint-config": "^4.0.0",
    "@splunk/webpack-configs": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-revision-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "node-fetch": "^3.3.0",
    "prop-types": "^15.8.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "shelljs": "^0.8.5",
    "styled-components": "5.1.1",
    "stylelint": "^13.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "peerDependencies": {
    "react": "^16.8",
    "styled-components": "5.1.1"
  },
  "engines": {
    "node": ">=6"
  }
